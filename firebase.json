{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/og",
        "function": "og"
      },
      {
        "source": "/og/**",
        "function": "og"
      }
    ]
  },
  "functions": {
    "source": "functions"
  }
}